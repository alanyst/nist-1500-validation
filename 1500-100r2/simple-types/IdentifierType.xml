<?xml version="1.0" encoding="UTF-8"?>
<validation_suite>
    <description>Tests for the IdentifierType data type.</description>
    <data_type>IdentifierType</data_type>
    <tests>
        <test name="set-membership">
            <description>Verifies that the value is in the enumerated set.</description>
            <valid>
                <value>fips</value>
                <value>local-level</value>
                <value>national-level</value>
                <value>ocd-id</value>
                <value>state-level</value>
                <value>other</value>
            </valid>
            <invalid>
                <value><!-- the empty string --></value>
                <value>local level</value>
                <value>FIPS</value>
                <value>some-arbitrary-value</value>
            </invalid>
        </test>
    </tests>
</validation_suite>
