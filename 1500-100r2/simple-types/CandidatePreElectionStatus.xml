<?xml version="1.0" encoding="UTF-8"?>
<validation_suite>
    <description>Tests for the CandidatePreElectionStatus data type.</description>
    <data_type>CandidatePreElectionStatus</data_type>
    <tests>
        <test name="set-membership">
            <description>Verifies that the value is in the enumerated set.</description>
            <valid>
                <value>filed</value>
                <value>qualified</value>
                <value>withdrawn</value>
            </valid>
            <invalid>
                <value><!-- the empty string --></value>
                <value>FILED</value>
                <value>some-arbitrary-value</value>
            </invalid>
        </test>
    </tests>
</validation_suite>
